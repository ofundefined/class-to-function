{
  "name": "class-to-function",
  "version": "1.0.11",
  "description": "Convert React class components to functional components.",
  "main": "./lib/cli.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/cli.ts",
    "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" src/cli.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "local": "npm i -g",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "bin": "./lib/cli.js",
  "keywords": [
    "react-native",
    "react",
    "class",
    "function",
    "convert"
  ],
  "author": "ofundefined",
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "chalk": "^5.0.0",
    "cli-table": "^0.3.11",
    "commander": "^9.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.0",
    "is-valid-glob": "^1.0.0",
    "prettier": "^2.6.0",
    "react": "^17.0.2",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.21",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
